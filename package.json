{
  "name": "flattrack",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start": "./flattrack",
    "start:frontend": "vue-cli-service serve ./src/frontend/main.js",
    "build:all": "npm run build:frontend && npm run build:api",
    "build:frontend": "vue-cli-service build --dest dist src/frontend/main.js",
    "build:frontend:watch": "vue-cli-service build --watch --dest dist src/frontend/main.js",
    "build:backend": "go build -o flattrack src/backend/main.go",
    "test:unit": "vue-cli-service test:unit --timeout 10000",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@mdi/font": "^4.9.95",
    "@mdi/js": "^5.4.55",
    "axios": "^0.19.2",
    "buefy": "^0.8.20",
    "core-js": "^3.6.5",
    "lodash": "^4.17.19",
    "material-icons": "^0.3.1",
    "moment": "^2.27.0",
    "node-emoji": "^1.10.0",
    "qrcode.vue": "^1.7.0",
    "register-service-worker": "^1.7.1",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.5",
    "vue-d3-network": "^0.1.28",
    "vue-json-viewer": "^2.2.14",
    "vue-material": "^1.0.0-beta-14",
    "vue-material-design-icons": "^4.8.0",
    "vue-property-decorator": "^8.5.1",
    "vue-router": "^3.3.4"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/lodash": "^4.14.158",
    "@types/mocha": "^5.2.4",
    "@vue/cli-plugin-babel": "^4.4.6",
    "@vue/cli-plugin-e2e-cypress": "^4.4.6",
    "@vue/cli-plugin-eslint": "^4.4.6",
    "@vue/cli-plugin-router": "^4.4.6",
    "@vue/cli-plugin-typescript": "^4.4.6",
    "@vue/cli-plugin-unit-mocha": "^4.4.6",
    "@vue/cli-service": "^4.4.6",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "chai": "^4.1.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^8.0.2",
