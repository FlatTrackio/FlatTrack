{
  "name": "flattrack",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start": "./flattrack",
    "start:frontend": "vue-cli-service serve ./src/frontend/main.js",
    "build:all": "npm run build:frontend && npm run build:api",
    "build:frontend": "vue-cli-service build --dest dist src/frontend/main.js",
    "build:frontend:watch": "vue-cli-service build --watch --dest dist src/frontend/main.js",
    "build:backend": "go build -o flattrack src/backend/main.go",
    "test:unit": "vue-cli-service test:unit --timeout 10000",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@mdi/font": "^4.9.95",
    "@mdi/js": "^5.1.45",
    "axios": "^0.19.2",
    "buefy": "^0.8.17",
    "core-js": "^3.6.5",
    "lodash": "^4.17.15",
    "material-icons": "^0.3.1",
    "moment": "^2.24.0",
    "node-emoji": "^1.10.0",
    "qrcode.vue": "^1.7.0",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-d3-network": "^0.1.28",
    "vue-json-viewer": "^2.2.11",
    "vue-material": "^1.0.0-beta-11",
    "vue-material-design-icons": "^4.6.0",
    "vue-property-decorator": "^8.4.1",
    "vue-router": "^3.1.6"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/lodash": "^4.14.150",
    "@types/mocha": "^5.2.4",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-e2e-cypress": "^4.3.1",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-plugin-router": "^4.3.1",
    "@vue/cli-plugin-typescript": "^4.3.1",
    "@vue/cli-plugin-unit-mocha": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "chai": "^4.1.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "typescript": "~3.5.3",
    "vue-template-compiler": "^2.6.11"
  }
